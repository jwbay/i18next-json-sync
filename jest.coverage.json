{
    "collectCoverage": true,
    "coverageReporters": ["lcov", "json", "text-summary"],
    "globals": { "COVERAGE": true },
    "moduleFileExtensions": ["js", "json", "ts"],
    "testEnvironment": "node",
    "roots": ["tests"],
    "testRegex": "runner.js$",
    "transform": {
        ".ts": "<rootDir>/testRunner/preprocessor.js"
    }
}